  <div class="container" ng-controller="AppCtrl">
    <h1>EXAM DETAILS</h1>

    <table class="table">
      <thead>
        <tr>
          <th>ID</th>
          <th>NAME</th> 
          <th>DOB</th>        
          <th>STANDARD</th>
          <th>MARKS</th>
          <th>Action</th>
          <th>&nbsp;</th>
        </tr>
      </thead>
      <tbody>
        <tr style="color:black">
          <td><input class="form-control" ng-model="stud.id"></td>
          <td><input class="form-control" ng-model="stud.username"></td>
          <td><input class="form-control" ng-model="stud.password"></td>
          <td><input class="form-control" ng-model="stud.standard"></td>
          <td><input class="form-control" ng-model="stud.marks"></td>
          <td><button class="btn btn-primary" ng-click="add()">Add</button></td>
          <td><button class="btn btn-info" ng-click="update()">Update</button>&nbsp;&nbsp;<button class="btn btn-info" ng-click="deselect()">Clear</button></td>
        </tr>
        <tr ng-repeat="stud in students" style="color:black">
          <td>{{stud.id}}</td>
          <td>{{stud.username}}</td>
          <td>{{stud.password}}</td>
          <td>{{stud.standard}}</td>
          <td>{{stud.marks}}</td>
          <td><button class="btn btn-danger" ng-click="remove(stud.id)">Remove</button></td>
          <td><button class="btn btn-warning" ng-click="edit(stud.id)">Edit</button></td>
        </tr>
      </tbody>
    </table>

  </div>
